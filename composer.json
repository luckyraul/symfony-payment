{
    "name": "luckyraul/symfony-payment",
    "authors": [
        {
            "name": "Nikita Tarasov",
            "email": "nikita@mygento.com"
        }
    ],
    "type": "symfony-bundle",
    "license": "proprietary",
    "description": "Mygento Payment Bundle",
    "prefer-stable": true,
    "require": {
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/orm": "^2.20 || ^3.0",
        "symfony/framework-bundle": "^7.0"
    },
    "require-dev": {
        "mygento/symfony-standard": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mygento\\Payment\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
          "php": "8.2.20"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}